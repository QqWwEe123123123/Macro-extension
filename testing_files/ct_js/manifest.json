{
    "manifest_version" : 2,
    "name" : "second",
    "version" : "0.1.0",

    "content_scripts": [
        {
        "matches" : [
            "<all_urls>"
        ],
        "js" : ["content.js"]
        }
    ],
    "background" : {
        "scripts" : ["background.js"]
    },
    "browser_action" : {
        "default_icon": "iostream.png",
        "default_popup" : "popup.html",
        "default_title" : "A popup will come here!"
    },
    "permissions": [
        "tabs",
        "https://www.google.com/*"
    ]
}